# Universe
universe        = vanilla

# Executable
executable      = job.sh

# Logs (make sure log/ exists)
log             = log/pythia.$(ClusterId).log
output          = log/pythia.$(ClusterId).$(ProcId).out
error           = log/pythia.$(ClusterId).$(ProcId).err


request_cpus    = 1
request_memory  = 2 GB
request_disk    = 2 GB

should_transfer_files = NO

environment = "CLUSTER_ID=$(ClusterId) PROC_ID=$(ProcId)"

queue 100
